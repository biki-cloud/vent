{
  "name": "vent-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "dev": "dotenv -e .env.dev -- next dev",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "db:seed": "npx prisma db seed",
    "prisma:seed": "tsx prisma/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "with-dev-env": "dotenv -e .env.dev --",
    "db:dev:generate": "pnpm with-dev-env prisma migrate dev",
    "db:dev:push": "pnpm with-dev-env prisma db push",
    "db:dev:studio": "pnpm with-dev-env prisma studio",
    "db:dev:reset": "pnpm with-dev-env prisma migrate reset --force && pnpm with-dev-env prisma db seed",
    "db:dev:seed": "pnpm with-dev-env prisma db seed",
    "db:prd:generate": "dotenv -e .env.prd -- prisma migrate dev",
    "db:prd:push": "dotenv -e .env.prd -- prisma db push",
    "db:prd:studio": "dotenv -e .env.prd -- prisma studio",
    "db:prd:seed": "dotenv -e .env.prd -- prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-select": "^2.1.5",
    "@radix-ui/react-slot": "^1.1.1",
    "@t3-oss/env-nextjs": "^0.10.1",
    "@tanstack/react-query": "^5.50.0",
    "@trpc/client": "11.0.0-rc.700",
    "@trpc/next": "^10.45.2",
    "@trpc/react-query": "^11.0.0-rc.446",
    "@trpc/server": "11.0.0-rc.700",
    "@types/uuid": "^10.0.0",
    "@vercel/analytics": "^1.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.0.4",
    "geist": "^1.3.0",
    "lucide-react": "^0.474.0",
    "next": "13.5.8",
    "next-pwa": "5.6.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "server-only": "^0.0.1",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.0.5",
    "zod": "^3.23.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/eslint": "^8.56.10",
    "@types/jest": "^29.5.14",
    "@types/next-pwa": "^5.6.9",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.14.0",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.5.3"
  },
  "ct3aMetadata": {
    "initVersion": "7.38.1"
  },
  "packageManager": "pnpm@9.15.4",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
